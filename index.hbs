{{!< default}}

<article class="gh-article">

    <header class="gh-header gh-canvas blog">
        <h1 class="gh-title">Onze blogs</h1>
        <p class="gh-excerpt">Een selectie van artikelen gemaakt door Terra 10 eindbazen</p>


    </header>

    <div class="gh-content gh-canvas">
        <div class="gh-postfeed">
            {{#foreach posts limit=3}}

                {{> "card"}} {{!-- partials/card.hbs --}}

            {{/foreach}}
        </div>
        {{pagination}}
    </div>
</article>
